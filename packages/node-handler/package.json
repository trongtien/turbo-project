{
  "name": "bds-node-handler",
  "version": "1.93.0",
  "description": "bds handler app back end",
  "main": "dist/index",
  "types": "dist/index.d.ts",
  "oclif": {
    "commands": "./dist/commands",
    "helpClass": "./dist/help",
    "bin": "bds"
  },
  "scripts": {
    "dev": "concurrently -k -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"npm run watch\" \"pnpm exec nodemon\"",
    "watch": "tsc-watch -p tsconfig.build.json --onCompilationComplete \"tsc-alias -p tsconfig.build.json\""
  },
  "bin": {
    "bds": "./bin/bds"
  },
  "keywords": [
    "bds",
    "setting"
  ],
  "engines": {
    "node": ">=18.17 <= 22"
  },
  "files": [
    "bin",
    "templates",
    "dist",
    "!dist/**/e2e.*"
  ],
  "devDependencies": {
    "@bds/typescript-config": "workspace:*"
  },
  "dependencies": {
    "bds-node-setting": "workspace:*",
    "@bds/node-core": "workspace:*",
    "@bds/node-decorator": "workspace:*"
  }
}
